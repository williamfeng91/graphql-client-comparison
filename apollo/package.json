{
  "name": "apollo",
  "version": "1.0.0",
  "private": true,
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "test": "bappo-scripts test",
    "test:update": "bappo-scripts test --updateSnapshot",
    "lint": "bappo-scripts lint",
    "format": "bappo-scripts format",
    "validate": "bappo-scripts validate",
    "codegen": "apollo client:codegen --target typescript --localSchemaFile ../schema.graphql --watch"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "edge 16"
    ],
    "production": [
      ">0.6%",
      "not dead",
      "not op_mini all",
      "not ie > 0"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "immer": "^6.0.2",
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "react-modal": "^3.11.2",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@bappo/scripts": "^1.3.0",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-modal": "^3.10.5",
    "@types/styled-components": "^5.0.1",
    "apollo": "^2.25.0",
    "react-scripts": "^3.4.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "bappo-scripts pre-commit"
    }
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
